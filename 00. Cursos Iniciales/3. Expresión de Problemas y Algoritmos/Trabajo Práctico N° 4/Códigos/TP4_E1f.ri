programa TP4_E1f
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    cant: numero
  comenzar
    cant:=0
    mientras (HayFlorEnLaEsquina & HayPapelEnLaEsquina)
      tomarFlor
      tomarPapel
      cant:=cant+1
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin
